@using CarDealership.Models;

<h1>Welcome to the Car Dealership!!</h1>
<ul>
@if (Model.Count == 0)
  {
    <p>No cars! go buy some.</p>
  }
@if (Model.Count != 0)
{
  @foreach(Car car in Model)
  {
    <li>@car.MakeModel, @car.Price, @car.Miles</li>
  }
}
</ul>
<a href="/car/form">Click here to list a new vehicle!</a>